<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- End current session if app sleeps for a period of time -->
    <integer name="ga_sessionTimeout">300</integer>

    <!-- Enable automatic Activity measurement -->
    <bool name="ga_autoActivityTracking">true</bool>

    <!--  The property id associated with this analytics tracker -->
    <string name="ga_trackingId">UA-76806433-2</string>

    <string name="ga_sampleFrequency">100.0</string>

    <bool name="ga_reportUncaughtExceptions">true</bool>

    <!--  Giving screen name for each activity to know that which screen is opened by user -->
    <screenName name="com.example.rahulkumarpandey.gasampleapp.MainActivity">Home Screen</screenName>
    <screenName name="com.example.rahulkumarpandey.gasampleapp.SecondActivity">Second Screen</screenName>
</resources>